[program:ls]
command=/bin/ls -l
numprocs=1
autostart=true
autorestart=true
exitcodes=1
startsecs=0
startretries=1
stopsignal=TERM
stopwaitsecs=10
stdout_logfile=/tmp/out
stderr_logfile=/tmp/err
environnement=HELLO="bonjour", WORLD="salut"
directory=/tmp
umask=000

[program:pwd]
command=/bin/pwd
numprocs=10000
autostart=true
autorestart=true
exitcodes=1
startsecs=0
startretries=1
stopsignal=TERM
stopwaitsecs=10
stdout_logfile=/tmp/out_4
stderr_logfile=/tmp/err
environnement=PWD="/Users/ariard"
directory=/tmp
umask=000

[program:date]
command=/bin/date
numprocs=1
autostart=false
autorestart=true
exitcodes=1
